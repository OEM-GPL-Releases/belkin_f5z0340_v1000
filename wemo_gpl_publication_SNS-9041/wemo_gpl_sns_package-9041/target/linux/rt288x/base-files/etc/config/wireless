config 'wifi-device' 'ra0'
        option 'type' 'ralink'
        option 'channel' 'auto'
        option 'disabled' '0'
        option 'country' 'US'
        option 'hwmode' 'auto'
        option 'htmode' 'HT40+'

config 'wifi-iface'
        option 'network' 'wifi_ap'
        option 'encryption' 'none'
        option 'key' '1234567890'
        option 'ssid' 'WeMo'
        option 'device' 'ra0'
        option 'mode' 'ap'

config 'wifi-device' 'apcli0'
        option 'type' 'ralink'
        option 'disabled' '1'

config 'wifi-iface'
        option 'network' 'lan'
        option 'encryption' 'none'
        option 'key' '1234567890'
        option 'ssid' 'Belkin'
        option 'device' 'apcli0'
        option 'mode' 'sta'
